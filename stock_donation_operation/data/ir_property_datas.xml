<openerp>
	<data>
		<record id="property_stock_donation_in" model="ir.property" forcecreate="True">
		    <field name="name">property_stock_donation_in_id</field>
		    <field name="fields_id" search="[('model','=','res.partner'),('name','=','property_stock_donation_in_id')]"/>
		    <field eval="'stock.location,'+str(stock_location_donation_in)" name="value"/>
		</record>

		<record id="property_stock_donation_out" model="ir.property" forcecreate="True">
		    <field name="name">property_stock_donation_out_id</field>
		    <field name="fields_id" search="[('model','=','res.partner'),('name','=','property_stock_donation_out_id')]"/>
		    <field eval="'stock.location,'+str(stock_location_donation_out)" name="value"/>
		</record>
	</data>
</openerp>

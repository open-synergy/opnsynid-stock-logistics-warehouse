<openerp>
<data>
<record id="property_stock_supplier_promotion" model="ir.property" forcecreate="True">
    <field name="name">property_stock_supplier_promotion_id</field>
    <field name="fields_id" search="[('model','=','res.partner'),('name','=','property_stock_supplier_promotion_id')]"/>
    <field eval="'stock.location,'+str(stock_location_supplier_promotion)" name="value"/>
</record>

<record id="property_stock_customer_promotion" model="ir.property" forcecreate="True">
    <field name="name">property_stock_customer_promotion_id</field>
    <field name="fields_id" search="[('model','=','res.partner'),('name','=','property_stock_customer_promotion_id')]"/>
    <field eval="'stock.location,'+str(stock_location_customer_promotion)" name="value"/>
</record>
</data>
</openerp>

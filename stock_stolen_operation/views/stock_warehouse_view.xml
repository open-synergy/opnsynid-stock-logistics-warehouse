<openerp>
    <data>

        <record id="stock_warehouse_view_form" model="ir.ui.view">
            <field name="name">Consume Operation</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wh_output_stock_loc_id']" position="before">
                    <label for="stolen_loc_id"/>
                    <div>
                        <field name="stolen_loc_id"/>
                        <button name="button_create_stolen_loc"
                            type="object"
                            string="(create)"
                            class="oe_link"
                            attrs="{'invisible': [('stolen_loc_id', '!=', False)]}"
                        />
                    </div>
                </xpath>
                <xpath expr="//field[@name='out_type_id']" position="after">
                    <label for="stolen_type_id"/>
                    <div>
                        <field name="stolen_type_id"/>
                        <button name="button_create_stolen_type"
                            type="object"
                            string="(create)"
                            class="oe_link"
                            attrs="{'invisible': [('stolen_type_id', '!=', False)]}"
                        />
                    </div>
                </xpath>
            </field>
        </record>
       
    </data>
</openerp>
